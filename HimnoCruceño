#include <iostream> // Libreria para escribir y leer

using namespace std; //Que la entrada va por el teclado y la salida es el monitor

int main()
{
    cout<<"Bajo el cielo más puro de América" << endl;
    cout<<"Y en la tierra de Ñuflo de Chávez" << endl;
    cout<<"Libertad van trinando las aves" << endl;
    cout<<"De sureste ostentando el primor" << endl;
    
    return 0;
}
